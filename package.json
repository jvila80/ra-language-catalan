{
  "name": "@joshfradley/ra-language-catalan",
  "description": "Catalan messages for react-admin, the frontend framework for building admin applications on top of REST/GraphQL services",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshf/ra-language-catalan.git"
  },
  "files": [
    "*.md",
    "lib",
    "esm",
    "src"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "watch": "tsc --outDir esm --module es2015 --watch"
  },
  "dependencies": {
    "ra-core": "^4.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "react",
    "react-admin",
    "admin-on-rest",
    "translation"
  ],
  "author": "Josh Fradley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshf/ra-language-catalan/issues"
  },
  "homepage": "https://github.com/joshf/ra-language-catalan#readme"
}
